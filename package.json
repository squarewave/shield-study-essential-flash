    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-istanbul": "^0.7.0",
    "grunt-shell": "^1.3.0",
    "istanbul-jpm": "^0.1.0",
    "jpm": "^1.0.7",
    "npm-run-all": "^3.1.0",
